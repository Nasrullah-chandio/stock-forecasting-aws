terraform {
  cloud {
    organization = "stock-forecast-aws"

    workspaces {
      name = "stock-forecast-cli"
    }
  }

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.0"
    }
  }

  required_version = ">= 1.3.0"
}

provider "aws" {
  region = var.aws_region
}
